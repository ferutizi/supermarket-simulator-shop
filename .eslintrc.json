{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "@typescript-eslint/member-delimiter-style": "off"
    },
    "overrides": [
        {
            "files": [
                "*.js",
                "*.jsx",
                "*.ts",
                "*.tsx"
            ]
        }
    ]
}
{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next",
        "standard-with-typescript",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/no-unescaped-entities": "off",
        "@next/next/no-page-custom-font": "off",
        "@typescript-eslint/member-delimiter-style": "off",
        "react/react-in-jsx-scope": "off"
    }
}